.body_menu
	#menu   
		.menu_item_wrap
			.menu_item.menu_item_active.Lang_item#EN(onclick='Lang_toggle("EN")') ENG
			.menu_item.Lang_item#RU(onclick='Lang_toggle("RU")') РУС
		.menu_item_wrap
			.menu_item.Lang_item#ZHO(onclick='Lang_toggle("ZHO")') 中文
			.menu_item.Lang_item#SPA(onclick='Lang_toggle("SPA")') Español
		.menu_item
		.menu_item#Language Language
		.menu_item.menu_item_header
		.menu_item#Theme Theme
		.menu_item_wrap
			.menu_item#Light(onclick='Theme_lite()') Light
			.menu_item#Dark(onclick='Theme_dark()') Dark
		.menu_item
		.menu_item.menu_item_header#Font_size Font size
		.menu_item_wrap
			.menu_item(onclick='Font_size(-1)') -
			.menu_item#Font_size_data
			.menu_item(onclick='Font_size(1)') +
		.menu_item
		.menu_item#Support Support
		.menu_item#Author Author

	#body_wrap.container(onclick='menu_off()')
		#body
