.text, .tag, .class, .text, .white
	white-space pre

.tag
	color #27e

.class
	color #8ef

.text
	color #eee

.white
	color #fff

.body_menu
	display flex
	height 100%

#body
	display flex
	flex-wrap wrap
	padding-top 88px

#menu
	display none
	margin-top 60px
	padding-top 10px
	padding-bottom 10px
	color #ccc
	background rgba(8, 8, 8, .8)

.see
	display block !important
	
.menu_item, .menu_item_wrap
	padding 5px 15px
	white-space nowrap
	cursor pointer
	box-sizing border-box
.menu_item_wrap
	display flex
	padding 0

.menu_item:hover, .menu_item_active
	background #000
	color #fff
.menu_item_active
	border 1px solid #eee