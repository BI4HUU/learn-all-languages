.text, .tag, .attribute_name, .text, .white
	white-space pre-wrap
.tag, .color_27e
	color #27e

.line
	display flex
	flex-wrap nowrap
	padding-right 15px
	padding-left 15px

.attribute_name, .color_8ef
	color #8ef

.comment, .color_888
	color #888
	
.text, .color_eee
	color #eee
	
.white, .color_fff
	color #fff

.attribute_value
	color #f06500

.b
	font-weight bold
.i
	font-style italic

.h1
	font-size 2rem
.h2
	font-size 1.5rem
.h3
	font-size 1.17rem
.h4
	font-size 1rem
.h5
	font-size .83rem
.h6
	font-size .67rem




.body_menu
	display flex
	height 100%
#body_wrap
	overflow auto
#body
	display flex
	flex-wrap wrap
	padding-top 88px

#menu
	display none
	margin-top 60px
	padding-top 10px
	padding-bottom 10px
	// overflow-y auto

.see
	display block !important
	
.menu_item, .menu_item_wrap
	padding 5px 15px
	white-space nowrap
	cursor pointer
	box-sizing border-box

.menu_item_wrap
	display flex
	padding 0


.end
	margin-left auto


.Theme_dark
	background #1e1e1e
	.text
		color #eee
	#menu
		color #ccc
		background rgba(8, 8, 8, .8)
	.menu_item_active
		border 1px solid #eee
	.menu_item:hover, .menu_item_active
		background #000
		color #fff
	.menu_item_header:hover
		color #ccc
		background rgba(8, 8, 8, .0)
.Theme_lite
	background #eeeeee
	.text
		color #222
	#menu
		color #333
		background rgba(222, 222, 222, .8)
	.menu_item_active
		border 1px solid #444
	.menu_item:hover, .menu_item_active
		background #fff
		color #000
	.attribute_name
		color #800000
	.menu_item_header:hover
		color #333
		background rgba(8, 8, 8, .0)
.menu_item_header
	padding 15px 15px 0 15px

.fs12
	font-size 12px