.text, .tag, .attribute_name, .text, .white
	white-space pre
.tag, .color_27e
	color #27e

.attribute_name, .color_8ef
	color #8ef

.text, .color_eee
	color #eee

.white, .color_fff
	color #fff
.attribute_value
	color #f06500

.body_menu
	display flex
	height 100%

#body
	display flex
	flex-wrap wrap
	padding-top 88px

#menu
	display none
	margin-top 60px
	padding-top 10px
	padding-bottom 10px

.see
	display block !important
	
.menu_item, .menu_item_wrap
	padding 5px 15px
	white-space nowrap
	cursor pointer
	box-sizing border-box

.menu_item_wrap
	display flex
	padding 0


.end
	margin-left auto


.Theme_dark
	background #1e1e1e
	.text
		color #eee
	#menu
		color #ccc
		background rgba(8, 8, 8, .8)
	.menu_item_active
		border 1px solid #eee
	.menu_item:hover, .menu_item_active
		background #000
		color #fff
.Theme_lite
	background #eeeeee
	.text
		color #333
	#menu
		color #333
		background rgba(222, 222, 222, .8)
	.menu_item_active
		border 1px solid #444
	.menu_item:hover, .menu_item_active
		background #fff
		color #000
	.attribute_name
		color #800000